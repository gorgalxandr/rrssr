.clearfix:before, 
.clearfix:after, 
.dl-horizontal dd:before, 
.dl-horizontal dd:after, 
.container:before, 
.container:after, 
.container-fluid:before, 
.container-fluid:after, 
.row:before, 
.row:after, 
.form-horizontal .form-group:before,
.form-horizontal .form-group:after,
.btn-toolbar:before,
.btn-toolbar:after,
.btn-group-vertical > .btn-group:before, 
.btn-group-vertical > .btn-group:after, 
.nav:before, 
.nav:after, 
.navbar:before, 
.navbar:after, 
.navbar-header:before, 
.navbar-header:after, 
.navbar-collapse:before, 
.navbar-collapse:after, 
.pager:before, 
.pager:after, 
.panel-body:before, 
.panel-body:after, 
.modal-header:before, 
.modal-header:after, 
.modal-footer:before, 
.modal-footer:after,
.btn-group-vertical > .btn-group-inverse:before, 
.btn-group-vertical > .btn-group-inverse:after, 
.nav-inverse:before, 
.nav-inverse:after
  content " "
  display table

*:before, 
*:after
  -webkit-box-sizing border-box
  box-sizing border-box

main   
  margin-top 72px
  padding-bottom 10px

ul
  list-style-type none

